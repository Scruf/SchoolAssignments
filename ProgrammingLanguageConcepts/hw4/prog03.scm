;start of the helpers
(define keep_handle(initial xs ys)
	(if (null? xs)
		#t
		(if (null? ys)
			#f
			(if (equal? (car xs)(car ys))
				(keep_handle initial (cdr xs)(cdr ys))
				(keep_handle initial initial (cdr ys))
			)
		)
	)
)
(define keep_sublist? (xs ys)
	(keep_handle xs xs ys)
)
;end of the helpers
;take 
(define take (n xs)
	(if (null? xs)
		xs
		(if (equal? n 0)
			'()
			(cons (car xs)(take(- n 1)(cdr xs))
		)
)))
