;sublist
(define sublist? (xs ys)
	(if (null? xs)
		#t
		(if (null? ys)
			#f
			(if (equal? (car xs)(car ys))
				(sublist? (cdr xs)(cdr ys))
				(sublist? xs (cdr ys))
			)
		)
	)
)
