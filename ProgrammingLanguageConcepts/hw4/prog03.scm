;insertion sort
(define insertion-sort (xs)
  (foldr (letrec 
             ((insert (lambda (next rest)
                        (if (null? rest)
                            (list1 next)
                            (if (< next (car rest))
                                (cons next rest)
                                (cons (car rest) (insert next (cdr rest))))))))
           insert)     
         '()
         xs))
